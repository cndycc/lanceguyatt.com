{
  "name": "lanceguyatt",
  "description": "Lance Guyatt website",
  "version": "1.0.0",
  "author": "Lance Guyatt <lance@lanceguyatt.com>",
  "homepage": "http://lanceguyatt.com",
  "dependencies": {
    "gatsby": "^2.1.37",
    "gatsby-image": "^2.0.34",
    "gatsby-plugin-google-analytics": "^2.0.17",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-nprogress": "^2.0.10",
    "gatsby-plugin-react-helmet": "^3.0.10",
    "gatsby-plugin-sharp": "^2.0.29",
    "gatsby-plugin-sitemap": "^2.0.10",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^2.0.27",
    "gatsby-source-strapi": "0.0.7",
    "gatsby-transformer-sharp": "^2.1.17",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-css-transition": "^0.7.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.3",
    "styled-system": "^4.0.5",
    "system-components": "^3.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "dotenv": "^7.0.0",
    "eslint": "^5.15.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-compat": "^3.0.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "prettier": "^1.16.4",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && gatsby build",
    "deploy": "GATSBY_SITE_URL=http://lanceguyatt.com npm run build && npm run surge",
    "surge": "surge -p public -d lanceguyatt.com",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "clean": "rm -rf public .cache",
    "format": "prettier --write \"./src/**/*.js\" \"./src/**/*.jsx\"",
    "lint:css": "stylelint \"./src/**/*.js\"",
    "diff": "prettier --config prettier.config.js --list-different \"**/*.js\" \"**/*.jsx\"",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ]
}
