{
  "name": "lanceguyatt",
  "version": "1.0.0",
  "description": "Personal site built with Contentful, ReactJS, GatsbyJS, GraphQL, and Styled Components. Inspired by Amiga Workbench 2.0.",
  "keywords": [
    "contentful",
    "react",
    "gatsby",
    "graphql",
    "styled-components",
    "amiga"
  ],
  "homepage": "https://github.com/lanceguyatt/lanceguyatt",
  "license": "MIT",
  "author": "Lance Guyatt <lance@lanceguyatt.com> (https://lanceguyatt.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/lanceguyatt/lanceguyatt.git"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "clean": "rm -rf public .cache",
    "format": "prettier --write \"./src/**/*.js\" \"./src/**/*.jsx\"",
    "lint:css": "stylelint \"./src/**/*.js\"",
    "diff": "prettier --config prettier.config.js --list-different \"**/*.js\" \"**/*.jsx\"",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "offline": "GATSBY_CONTENTFUL_OFFLINE=true yarn run dev",
    "deploy": "npm run clean && npm run build && netlify deploy",
    "deploy:prod": "npm run clean && npm run build && netlify deploy --prod"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.0",
    "@mdx-js/react": "^1.5.0",
    "@rebass/components": "^4.0.0-1",
    "@styled-system/css": "^5.0.23",
    "dotenv": "^8.1.0",
    "eslint-plugin-react-hooks": "^2.0.1",
    "gatsby": "^2.15.28",
    "gatsby-image": "^2.2.23",
    "gatsby-plugin-canonical-urls": "^2.1.10",
    "gatsby-plugin-google-analytics": "^2.1.19",
    "gatsby-plugin-manifest": "^2.2.20",
    "gatsby-plugin-mdx": "^1.0.46",
    "gatsby-plugin-nprogress": "^2.1.9",
    "gatsby-plugin-offline": "^3.0.11",
    "gatsby-plugin-react-helmet": "^3.1.10",
    "gatsby-plugin-sitemap": "^2.2.16",
    "gatsby-plugin-styled-components": "^3.1.8",
    "gatsby-remark-prismjs": "^3.3.16",
    "gatsby-source-contentful": "^2.1.44",
    "polished": "^3.4.1",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-css-transition": "^0.7.4",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.4.0",
    "styled-system": "^5.1.2",
    "system-components": "^3.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "prettier": "^1.18.2",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0"
  },
  "private": true,
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ]
}
