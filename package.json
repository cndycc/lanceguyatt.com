{
  "name": "lanceguyatt",
  "version": "1.0.0",
  "description": "Personal site built with Contentful, ReactJS, GatsbyJS, GraphQL, and Styled Components. Inspired by Amiga Workbench 2.0.",
  "keywords": [
    "contentful",
    "react",
    "gatsby",
    "graphql",
    "styled-components",
    "amiga"
  ],
  "homepage": "https://github.com/lanceguyatt/lanceguyatt",
  "license": "MIT",
  "author": "Lance Guyatt <lance@lanceguyatt.com> (https://lanceguyatt.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/lanceguyatt/lanceguyatt.git"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "clean": "rm -rf public .cache",
    "format": "prettier --write \"./src/**/*.js\" \"./src/**/*.jsx\"",
    "lint:css": "stylelint \"./src/**/*.js\"",
    "diff": "prettier --config prettier.config.js --list-different \"**/*.js\" \"**/*.jsx\"",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "offline": "GATSBY_CONTENTFUL_OFFLINE=true yarn run dev",
    "deploy": "npm run clean && npm run build && netlify deploy",
    "deploy:prod": "npm run clean && npm run build && netlify deploy --prod"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.16",
    "@mdx-js/react": "^1.6.16",
    "@rebass/components": "^4.0.0-1",
    "@styled-system/css": "^5.1.5",
    "dotenv": "^8.2.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gatsby": "^2.24.23",
    "gatsby-image": "^2.4.14",
    "gatsby-plugin-canonical-urls": "^2.3.10",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-manifest": "^2.4.21",
    "gatsby-plugin-mdx": "^1.2.30",
    "gatsby-plugin-nprogress": "^2.3.10",
    "gatsby-plugin-offline": "^3.2.21",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-remark-prismjs": "^3.5.10",
    "gatsby-source-contentful": "^2.3.34",
    "polished": "^3.6.5",
    "prismjs": "^1.20.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-css-transition": "^0.7.4",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.1.1",
    "styled-system": "^5.1.5",
    "system-components": "^3.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "private": true,
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ]
}
