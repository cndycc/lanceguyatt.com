{
  "name": "lanceguyatt",
  "version": "1.0.0",
  "description": "Personal site built with Contentful, ReactJS, GatsbyJS, GraphQL, and Styled Components. Inspired by Amiga Workbench 2.0.",
  "keywords": [
    "contentful",
    "react",
    "gatsby",
    "graphql",
    "styled-components",
    "amiga"
  ],
  "homepage": "https://github.com/lanceguyatt/lanceguyatt",
  "license": "MIT",
  "author": "Lance Guyatt <lance@lanceguyatt.com> (https://lanceguyatt.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/lanceguyatt/lanceguyatt.git"
  },
  "scripts": {
    "build": "npm run clean && gatsby build",
    "dev": "gatsby develop",
    "clean": "rm -rf public .cache",
    "format": "prettier --write \"./src/**/*.js\" \"./src/**/*.jsx\"",
    "lint:css": "stylelint \"./src/**/*.js\"",
    "diff": "prettier --config prettier.config.js --list-different \"**/*.js\" \"**/*.jsx\"",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "offline": "GATSBY_CONTENTFUL_OFFLINE=true yarn run dev"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.0.21",
    "@mdx-js/react": "^1.0.21",
    "@mdx-js/tag": "^0.20.3",
    "dotenv": "^8.0.0",
    "gatsby": "^2.11.7",
    "gatsby-image": "^2.2.3",
    "gatsby-mdx": "^0.6.3",
    "gatsby-plugin-canonical-urls": "^2.1.0",
    "gatsby-plugin-google-analytics": "^2.1.1",
    "gatsby-plugin-manifest": "^2.2.0",
    "gatsby-plugin-nprogress": "^2.1.0",
    "gatsby-plugin-offline": "^2.2.0",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sitemap": "^2.2.1",
    "gatsby-plugin-styled-components": "^3.1.0",
    "gatsby-remark-prismjs": "^3.3.0",
    "gatsby-source-contentful": "^2.1.4",
    "polished": "^3.4.1",
    "prismjs": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-css-transition": "^0.7.4",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.3.2",
    "styled-system": "^5.0.12",
    "system-components": "^3.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-compat": "^3.2.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0"
  },
  "private": true,
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ]
}
