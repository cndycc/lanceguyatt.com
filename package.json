{
  "name": "lanceguyatt",
  "version": "1.0.0",
  "description": "Personal site built with Contentful, ReactJS, GatsbyJS, GraphQL, and Styled Components. Inspired by Amiga Workbench 2.0.",
  "keywords": [
    "contentful",
    "react",
    "gatsby",
    "graphql",
    "styled-components",
    "amiga"
  ],
  "homepage": "https://github.com/lanceguyatt/lanceguyatt",
  "license": "MIT",
  "author": "Lance Guyatt <lance@lanceguyatt.com> (https://lanceguyatt.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/lanceguyatt/lanceguyatt.git"
  },
  "scripts": {
    "build": "npm run clean && gatsby build",
    "dev": "gatsby develop",
    "clean": "rm -rf public .cache",
    "format": "prettier --write \"./src/**/*.js\" \"./src/**/*.jsx\"",
    "lint:css": "stylelint \"./src/**/*.js\"",
    "diff": "prettier --config prettier.config.js --list-different \"**/*.js\" \"**/*.jsx\"",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "offline": "GATSBY_CONTENTFUL_OFFLINE=true yarn run dev"
  },
  "dependencies": {
    "dotenv": "^7.0.0",
    "gatsby": "^2.3.27",
    "gatsby-image": "^2.0.40",
    "gatsby-plugin-canonical-urls": "^2.0.12",
    "gatsby-plugin-google-analytics": "^2.0.18",
    "gatsby-plugin-manifest": "^2.0.29",
    "gatsby-plugin-nprogress": "^2.0.10",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sitemap": "^2.0.12",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-source-contentful": "^2.0.52",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-css-transition": "^0.7.4",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.2.0",
    "styled-system": "^4.1.0",
    "system-components": "^3.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "prettier": "^1.17.0",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.6.0"
  },
  "private": true,
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ]
}
